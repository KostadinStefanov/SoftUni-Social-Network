app.controller("AppController", ["$scope", "$route", "$location", "AccountService", "notification",
    function ($scope, $route, $location, AccountService, notification) {
        $scope.$route = $route;
        $scope.AccountService = AccountService;

        $scope.logout = function () {
            AccountService.logout();
            NotificationService.displaySuccessMessage("Logout successful.");
            $location.path("#/");
        }
    }]);